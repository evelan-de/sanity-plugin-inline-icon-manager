{
  "name": "sanity-plugin-inline-icon-manager",
  "version": "1.4.0",
  "description": "A Sanity plugin for selecting, managing, and customizing icons. Based on sanity-plugin-icon-manager, but can be configured to use the inline svg by default",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "sanity-plugin-icon-manager",
    "sanity-plugin-inline-icon-manager"
  ],
  "homepage": "https://github.com/evelan-de/sanity-plugin-inline-icon-manager#readme",
  "bugs": {
    "url": "https://github.com/evelan-de/sanity-plugin-inline-icon-manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/evelan-de/sanity-plugin-inline-icon-manager.git"
  },
  "license": "MIT",
  "author": "Evelan",
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./types": {
      "source": "./src/exports/types/index.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "default": "./dist/types.mjs"
    },
    "./renderer": {
      "source": "./src/exports/components/IconInlineRenderer/index.ts",
      "import": "./dist/renderer.mjs",
      "require": "./dist/renderer.js",
      "default": "./dist/renderer.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types.d.ts"
      ],
      "renderer": [
        "./dist/renderer.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "renderer.js",
    "types.js",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/openai": "1.3.23",
    "@iconify/react": "4.1.1",
    "@iconify/utils": "2.1.9",
    "@sanity/incompatible-plugin": "1.0.5",
    "@sanity/studio-secrets": "3.0.2",
    "@sanity/ui": "2.16.7",
    "ai": "4.3.19",
    "html-react-parser": "5.2.6",
    "isomorphic-dompurify": "2.26.0",
    "react-colorful": "5.6.1",
    "zod": "3.23.8",
    "zustand": "4.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@iconify/types": "2.0.0",
    "@sanity/pkg-utils": "7.9.7",
    "@sanity/plugin-kit": "4.0.19",
    "@sanity/semantic-release-preset": "5.0.0",
    "@types/react": "18.3.3",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "8.37.0",
    "@typescript-eslint/parser": "8.37.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-config-sanity": "7.1.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.1",
    "prettier-plugin-packagejson": "2.4.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-is": "18.2.0",
    "rimraf": "5.0.1",
    "sanity": "4.1.0",
    "styled-components": "6.1.19",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "sanity": "^3 || ^4"
  },
  "engines": {
    "node": ">=18"
  }
}
